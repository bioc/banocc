@article{KuramitsuEtAl2007,
	Annote = {10.1128/MMBR.00024-07},
	Author = {Kuramitsu, Howard K. and He, Xuesong and Lux, Renate and Anderson, Maxwell H. and Shi, Wenyuan},
	Date-Added = {2015-09-28 14:31:00 +0000},
	Date-Modified = {2016-05-25 13:43:27 +0000},
	Journal = {Microbiology and Molecular Biology Reviews},
	Journal1 = {Microbiology and Molecular Biology Reviews},
	Keywords = {Paper2; Microbial Interactions; To Read},
	Month = {12},
	Number = {4},
	Pages = {653--670},
	Title = {Interspecies Interactions within Oral Microbial Communities},
	Ty = {JOUR},
	Url = {http://mmbr.asm.org/content/71/4/653.abstract N2 - Summary: While reductionism has greatly advanced microbiology in the past 400 years, assembly of smaller pieces just could not explain the whole! Modern microbiologists are learning “system thinking” and “holism.” Such an approach is changing our understanding of microbial physiology and our ability to diagnose/treat microbial infections. This review uses oral microbial communities as a focal point to describe this new trend. With the common name “dental plaque,” oral microbial communities are some of the most complex microbial floras in the human body, consisting of more than 700 different bacterial species. For a very long time, oral microbiologists endeavored to use reductionism to identify the key genes or key pathogens responsible for oral microbial pathogenesis. The limitations of reductionism forced scientists to begin adopting new strategies using emerging concepts such as interspecies interaction, microbial community, biofilms, polymicrobial disease, etc. These new research directions indicate that the whole is much more than the simple sum of its parts, since the interactions between different parts resulted in many new physiological functions which cannot be observed with individual components. This review describes some of these interesting interspecies-interaction scenarios.},
	Url2 = {http://mmbr.asm.org.ezp-prod1.hul.harvard.edu/content/71/4/653.short},
	Volume = {71},
	Year = {2007},
	Bdsk-Url-1 = {http://mmbr.asm.org/content/71/4/653.abstract%20N2%20-%20Summary:%20While%20reductionism%20has%20greatly%20advanced%20microbiology%20in%20the%20past%20400%20years,%20assembly%20of%20smaller%20pieces%20just%20could%20not%20explain%20the%20whole!%20Modern%20microbiologists%20are%20learning%20%E2%80%9Csystem%20thinking%E2%80%9D%20and%20%E2%80%9Cholism.%E2%80%9D%20Such%20an%20approach%20is%20changing%20our%20understanding%20of%20microbial%20physiology%20and%20our%20ability%20to%20diagnose/treat%20microbial%20infections.%20This%20review%20uses%20oral%20microbial%20communities%20as%20a%20focal%20point%20to%20describe%20this%20new%20trend.%20With%20the%20common%20name%20%E2%80%9Cdental%20plaque,%E2%80%9D%20oral%20microbial%20communities%20are%20some%20of%20the%20most%20complex%20microbial%20floras%20in%20the%20human%20body,%20consisting%20of%20more%20than%20700%20different%20bacterial%20species.%20For%20a%20very%20long%20time,%20oral%20microbiologists%20endeavored%20to%20use%20reductionism%20to%20identify%20the%20key%20genes%20or%20key%20pathogens%20responsible%20for%20oral%20microbial%20pathogenesis.%20The%20limitations%20of%20reductionism%20forced%20scientists%20to%20begin%20adopting%20new%20strategies%20using%20emerging%20concepts%20such%20as%20interspecies%20interaction,%20microbial%20community,%20biofilms,%20polymicrobial%20disease,%20etc.%20These%20new%20research%20directions%20indicate%20that%20the%20whole%20is%20much%20more%20than%20the%20simple%20sum%20of%20its%20parts,%20since%20the%20interactions%20between%20different%20parts%20resulted%20in%20many%20new%20physiological%20functions%20which%20cannot%20be%20observed%20with%20individual%20components.%20This%20review%20describes%20some%20of%20these%20interesting%20interspecies-interaction%20scenarios.}}

