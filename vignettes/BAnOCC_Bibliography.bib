@article{LiuEtAl2015,
	Annote = {10.1093/bioinformatics/btu778},
	Author = {Liu, Zhenqiu and Sun, Fengzhu and Braun, Jonathan and McGovern, Dermot P. B. and Piantadosi, Steven},
	Date-Added = {2015-09-10 12:46:24 +0000},
	Date-Modified = {2015-09-11 15:58:39 +0000},
	Journal = {Bioinformatics},
	Journal1 = {Bioinformatics},
	Keywords = {To Read},
	Month = {04},
	Number = {7},
	Pages = {1067--1074},
	Title = {Multilevel regularized regression for simultaneous taxa selection and network construction with metagenomic count data},
	Ty = {JOUR},
	Url = {http://bioinformatics.oxfordjournals.org/content/31/7/1067.abstract N2 - Motivation: Identifying disease associated taxa and constructing networks for bacteria interactions are two important tasks usually studied separately. In reality, differentiation of disease associated taxa and correlation among taxa may affect each other. One genus can be differentiated because it is highly correlated with another highly differentiated one. In addition, network structures may vary under different clinical conditions. Permutation tests are commonly used to detect differences between networks in distinct phenotypes, and they are time-consuming.Results: In this manuscript, we propose a multilevel regularized regression method to simultaneously identify taxa and construct networks. We also extend the framework to allow construction of a common network and differentiated network together. An efficient algorithm with dual formulation is developed to deal with the large-scale n ≪ m problem with a large number of taxa (m) and a small number of samples (n) efficiently. The proposed method is regularized with a general Lp (p∈[0,2]) penalty and models the effects of taxa abundance differentiation and correlation jointly. We demonstrate that it can identify both true and biologically significant genera and network structures.Availability and implementation: Software MLRR in MATLAB is available at http://biostatistics.csmc.edu/mlrr/.Contact: liuzx@cshs.orgSupplementary information: Supplementary data are available at Bioinformatics online.},
	Volume = {31},
	Year = {2015},
	Bdsk-Url-1 = {http://bioinformatics.oxfordjournals.org/content/31/7/1067.abstract%20N2%20-%20Motivation:%20Identifying%20disease%20associated%20taxa%20and%20constructing%20networks%20for%20bacteria%20interactions%20are%20two%20important%20tasks%20usually%20studied%20separately.%20In%20reality,%20differentiation%20of%20disease%20associated%20taxa%20and%20correlation%20among%20taxa%20may%20affect%20each%20other.%20One%20genus%20can%20be%20differentiated%20because%20it%20is%20highly%20correlated%20with%20another%20highly%20differentiated%20one.%20In%20addition,%20network%20structures%20may%20vary%20under%20different%20clinical%20conditions.%20Permutation%20tests%20are%20commonly%20used%20to%20detect%20differences%20between%20networks%20in%20distinct%20phenotypes,%20and%20they%20are%20time-consuming.Results:%20In%20this%20manuscript,%20we%20propose%20a%20multilevel%20regularized%20regression%20method%20to%20simultaneously%20identify%20taxa%20and%20construct%20networks.%20We%20also%20extend%20the%20framework%20to%20allow%20construction%20of%20a%20common%20network%20and%20differentiated%20network%20together.%20An%20efficient%20algorithm%20with%20dual%20formulation%20is%20developed%20to%20deal%20with%20the%20large-scale%20n%20%E2%89%AA%20m%20problem%20with%20a%20large%20number%20of%20taxa%20(m)%20and%20a%20small%20number%20of%20samples%20(n)%20efficiently.%20The%20proposed%20method%20is%20regularized%20with%20a%20general%20Lp%20(p%E2%88%88%5B0,2%5D)%20penalty%20and%20models%20the%20effects%20of%20taxa%20abundance%20differentiation%20and%20correlation%20jointly.%20We%20demonstrate%20that%20it%20can%20identify%20both%20true%20and%20biologically%20significant%20genera%20and%20network%20structures.Availability%20and%20implementation:%20Software%20MLRR%20in%20MATLAB%20is%20available%20at%20http://biostatistics.csmc.edu/mlrr/.Contact:%20liuzx@cshs.orgSupplementary%20information:%20Supplementary%20data%20are%20available%20at%20Bioinformatics%20online.}}

@article{MorfopouluAndPlagnol2015,
	Annote = {10.1093/bioinformatics/btv317},
	Author = {Morfopoulou, Sofia and Plagnol, Vincent},
	Date-Added = {2015-09-10 12:43:16 +0000},
	Date-Modified = {2015-09-11 15:59:08 +0000},
	Journal = {Bioinformatics},
	Journal1 = {Bioinformatics},
	Keywords = {To Read},
	Month = {09},
	Number = {18},
	Pages = {2930--2938},
	Title = {Bayesian mixture analysis for metagenomic community profiling},
	Ty = {JOUR},
	Url = {http://bioinformatics.oxfordjournals.org/content/31/18/2930.abstract N2 - Motivation: Deep sequencing of clinical samples is now an established tool for the detection of infectious pathogens, with direct medical applications. The large amount of data generated produces an opportunity to detect species even at very low levels, provided that computational tools can effectively profile the relevant metagenomic communities. Data interpretation is complicated by the fact that short sequencing reads can match multiple organisms and by the lack of completeness of existing databases, in particular for viral pathogens. Here we present metaMix, a Bayesian mixture model framework for resolving complex metagenomic mixtures. We show that the use of parallel Monte Carlo Markov chains for the exploration of the species space enables the identification of the set of species most likely to contribute to the mixture.Results: We demonstrate the greater accuracy of metaMix compared with relevant methods, particularly for profiling complex communities consisting of several related species. We designed metaMix specifically for the analysis of deep transcriptome sequencing datasets, with a focus on viral pathogen detection; however, the principles are generally applicable to all types of metagenomic mixtures.Availability and implementation: metaMix is implemented as a user friendly R package, freely available on CRAN: http://cran.r-project.org/web/packages/metaMixContact: sofia.morfopoulou.10@ucl.ac.ukSupplementary information: Supplementary data are available at Bionformatics online.},
	Volume = {31},
	Year = {2015},
	Bdsk-Url-1 = {http://bioinformatics.oxfordjournals.org/content/31/18/2930.abstract%20N2%20-%20Motivation:%20Deep%20sequencing%20of%20clinical%20samples%20is%20now%20an%20established%20tool%20for%20the%20detection%20of%20infectious%20pathogens,%20with%20direct%20medical%20applications.%20The%20large%20amount%20of%20data%20generated%20produces%20an%20opportunity%20to%20detect%20species%20even%20at%20very%20low%20levels,%20provided%20that%20computational%20tools%20can%20effectively%20profile%20the%20relevant%20metagenomic%20communities.%20Data%20interpretation%20is%20complicated%20by%20the%20fact%20that%20short%20sequencing%20reads%20can%20match%20multiple%20organisms%20and%20by%20the%20lack%20of%20completeness%20of%20existing%20databases,%20in%20particular%20for%20viral%20pathogens.%20Here%20we%20present%20metaMix,%20a%20Bayesian%20mixture%20model%20framework%20for%20resolving%20complex%20metagenomic%20mixtures.%20We%20show%20that%20the%20use%20of%20parallel%20Monte%20Carlo%20Markov%20chains%20for%20the%20exploration%20of%20the%20species%20space%20enables%20the%20identification%20of%20the%20set%20of%20species%20most%20likely%20to%20contribute%20to%20the%20mixture.Results:%20We%20demonstrate%20the%20greater%20accuracy%20of%20metaMix%20compared%20with%20relevant%20methods,%20particularly%20for%20profiling%20complex%20communities%20consisting%20of%20several%20related%20species.%20We%20designed%20metaMix%20specifically%20for%20the%20analysis%20of%20deep%20transcriptome%20sequencing%20datasets,%20with%20a%20focus%20on%20viral%20pathogen%20detection;%20however,%20the%20principles%20are%20generally%20applicable%20to%20all%20types%20of%20metagenomic%20mixtures.Availability%20and%20implementation:%20metaMix%20is%20implemented%20as%20a%20user%20friendly%20R%20package,%20freely%20available%20on%20CRAN:%20http://cran.r-project.org/web/packages/metaMixContact:%20sofia.morfopoulou.10@ucl.ac.ukSupplementary%20information:%20Supplementary%20data%20are%20available%20at%20Bionformatics%20online.}}

@article{LiechtyEtAl2004,
	Annote = {We propose prior probability models for variance-covariance matrices in order to address two important issues. First, the models allow a researcher to represent substantive prior information about the strength of correlations among a set of variables. Secondly, even in the absence of such information, the increased flexibility of the models mitigates dependence on strict parametric assumptions in standard prior models. For example, the model allows a posteriori different levels of uncertainty about correlations among different subsets of variables. We achieve this by including a clustering mechanism in the prior probability model. Clustering is with respect to variables and pairs of variables. Our approach leads to shrinkage towards a mixture structure implied by the clustering. We discuss appropriate posterior simulation schemes to implement posterior inference in the proposed models, including the evaluation of normalising constants that are functions of parameters of interest. The normalising constants result from the restriction that the correlation matrix be positive definite. We discuss examples based on simulated data, a stock return dataset and a population genetics dataset.},
