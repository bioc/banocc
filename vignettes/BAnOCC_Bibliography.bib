@article{BergerAndSellke1987,
	Annote = {The problem of testing a point null hypothesis (or a "small interval" null hypothesis) is considered. Of interest is the relationship between the P value (or observed significance level) and conditional and Bayesian measures of evidence against the null hypothesis. Although one might presume that a small P value indicates the presence of strong evidence against the null, such is not necessarily the case. Expanding on earlier work {$[$}especially Edwards, Lindman, and Savage (1963) and Dickey (1977){$]$}, it is shown that actual evidence against a null (as measured, say, by posterior probability or comparative likelihood) can differ by an order of magnitude from the P value. For instance, data that yield a P value of .05, when testing a normal mean, result in a posterior probability of the null of at least .30 for any objective prior distribution. ("Objective" here means that equal prior weight is given the two hypotheses and that the prior is symmetric and nonincreasing away from the null; other definitions of "objective" will be seen to yield qualitatively similar results.) The overall conclusion is that P values can be highly misleading measures of the evidence provided by the data against the null hypothesis.},
	Author = {Berger, James O. and Sellke, Thomas},
	Cr = {Copyright \copyright 1987 American Statistical Association},
	Date-Added = {2015-09-09 15:11:29 +0000},
	Date-Modified = {2016-06-21 13:39:27 +0000},
	Doi = {10.2307/2289131},
	Isbn = {01621459},
	Journal = {Journal of the American Statistical Association},
	Keywords = {Paper1; Bayesian inference; Paper2},
	M1 = {ArticleType: research-article / Full publication date: Mar., 1987 / Copyright {\copyright}1987 American Statistical Association},
	Month = {03},
	Number = {397},
	Pages = {112--122},
	Publisher = {Taylor \& Francis, Ltd. on behalf of the American Statistical Association},
	Title = {Testing a Point Null Hypothesis: The Irreconcilability of P Values and Evidence},
	Ty = {JOUR},
