	Title = {Power and sample-size estimation for microbiome studies using pairwise distances and PERMANOVA},
	Ty = {JOUR},
	Url = {http://bioinformatics.oxfordjournals.org/content/31/15/2461},
	Volume = {31},
	Year = {2015},
	Bdsk-Url-1 = {http://bioinformatics.oxfordjournals.org/content/31/15/2461.abstract%20N2%20-%20Motivation:%20The%20variation%20in%20community%20composition%20between%20microbiome%20samples,%20termed%20beta%20diversity,%20can%20be%20measured%20by%20pairwise%20distance%20based%20on%20either%20presence%E2%80%93absence%20or%20quantitative%20species%20abundance%20data.%20PERMANOVA,%20a%20permutation-based%20extension%20of%20multivariate%20analysis%20of%20variance%20to%20a%20matrix%20of%20pairwise%20distances,%20partitions%20within-group%20and%20between-group%20distances%20to%20permit%20assessment%20of%20the%20effect%20of%20an%20exposure%20or%20intervention%20(grouping%20factor)%20upon%20the%20sampled%20microbiome.%20Within-group%20distance%20and%20exposure/intervention%20effect%20size%20must%20be%20accurately%20modeled%20to%20estimate%20statistical%20power%20for%20a%20microbiome%20study%20that%20will%20be%20analyzed%20with%20pairwise%20distances%20and%20PERMANOVA.Results:%20We%20present%20a%20framework%20for%20PERMANOVA%20power%20estimation%20tailored%20to%20marker-gene%20microbiome%20studies%20that%20will%20be%20analyzed%20by%20pairwise%20distances,%20which%20includes:%20(i)%20a%20novel%20method%20for%20distance%20matrix%20simulation%20that%20permits%20modeling%20of%20within-group%20pairwise%20distances%20according%20to%20pre-specified%20population%20parameters;%20(ii)%20a%20method%20to%20incorporate%20effects%20of%20different%20sizes%20within%20the%20simulated%20distance%20matrix;%20(iii)%20a%20simulation-based%20method%20for%20estimating%20PERMANOVA%20power%20from%20simulated%20distance%20matrices;%20and%20(iv)%20an%20R%20statistical%20software%20package%20that%20implements%20the%20above.%20Matrices%20of%20pairwise%20distances%20can%20be%20efficiently%20simulated%20to%20satisfy%20the%20triangle%20inequality%20and%20incorporate%20group-level%20effects,%20which%20are%20quantified%20by%20the%20adjusted%20coefficient%20of%20determination,%20omega-squared%20(%CF%892).%20From%20simulated%20distance%20matrices,%20available%20PERMANOVA%20power%20or%20necessary%20sample%20size%20can%20be%20estimated%20for%20a%20planned%20microbiome%20study.Availability%20and%20implementation:%20http://github.com/brendankelly/micropower.Contact:%20brendank@mail.med.upenn.edu%20or%20hongzhe@upenn.edu}}

@article{FreitasEtAl2015,
	Annote = {10.1093/nar/gkv180},
	Author = {Freitas, Tracey Allen K. and Li, Po-E and Scholz, Matthew B. and Chain, Patrick S. G.},
	Date-Added = {2015-09-10 12:51:25 +0000},
	Date-Modified = {2015-09-11 15:56:55 +0000},
	Journal = {Nucleic Acids Research},
	Journal1 = {Nucleic Acids Research},
	Keywords = {To Read},
	Month = {03},
	Title = {Accurate read-based metagenome characterization using a hierarchical suite of unique signatures},
	Ty = {JOUR},
	Url = {http://nar.oxfordjournals.org/content/early/2015/03/12/nar.gkv180.abstract N2 - A major challenge in the field of shotgun metagenomics is the accurate identification of organisms present within a microbial community, based on classification of short sequence reads. Though existing microbial community profiling methods have attempted to rapidly classify the millions of reads output from modern sequencers, the combination of incomplete databases, similarity among otherwise divergent genomes, errors and biases in sequencing technologies, and the large volumes of sequencing data required for metagenome sequencing has led to unacceptably high false discovery rates (FDR). Here, we present the application of a novel, gene-independent and signature-based metagenomic taxonomic profiling method with significantly and consistently smaller FDR than any other available method. Our algorithm circumvents false positives using a series of non-redundant signature databases and examines Genomic Origins Through Taxonomic CHAllenge (GOTTCHA). GOTTCHA was tested and validated on 20 synthetic and mock datasets ranging in community composition and complexity, was applied successfully to data generated from spiked environmental and clinical samples, and robustly demonstrates superior performance compared with other available tools.},
	Year = {2015},
	Bdsk-Url-1 = {http://nar.oxfordjournals.org/content/early/2015/03/12/nar.gkv180.abstract%20N2%20-%20A%20major%20challenge%20in%20the%20field%20of%20shotgun%20metagenomics%20is%20the%20accurate%20identification%20of%20organisms%20present%20within%20a%20microbial%20community,%20based%20on%20classification%20of%20short%20sequence%20reads.%20Though%20existing%20microbial%20community%20profiling%20methods%20have%20attempted%20to%20rapidly%20classify%20the%20millions%20of%20reads%20output%20from%20modern%20sequencers,%20the%20combination%20of%20incomplete%20databases,%20similarity%20among%20otherwise%20divergent%20genomes,%20errors%20and%20biases%20in%20sequencing%20technologies,%20and%20the%20large%20volumes%20of%20sequencing%20data%20required%20for%20metagenome%20sequencing%20has%20led%20to%20unacceptably%20high%20false%20discovery%20rates%20(FDR).%20Here,%20we%20present%20the%20application%20of%20a%20novel,%20gene-independent%20and%20signature-based%20metagenomic%20taxonomic%20profiling%20method%20with%20significantly%20and%20consistently%20smaller%20FDR%20than%20any%20other%20available%20method.%20Our%20algorithm%20circumvents%20false%20positives%20using%20a%20series%20of%20non-redundant%20signature%20databases%20and%20examines%20Genomic%20Origins%20Through%20Taxonomic%20CHAllenge%20(GOTTCHA).%20GOTTCHA%20was%20tested%20and%20validated%20on%2020%20synthetic%20and%20mock%20datasets%20ranging%20in%20community%20composition%20and%20complexity,%20was%20applied%20successfully%20to%20data%20generated%20from%20spiked%20environmental%20and%20clinical%20samples,%20and%20robustly%20demonstrates%20superior%20performance%20compared%20with%20other%20available%20tools.}}

