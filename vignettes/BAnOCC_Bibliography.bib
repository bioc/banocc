	Month = {02},
	Number = {2},
	Pages = {e47879 EP -},
	Publisher = {Public Library of Science},
	Title = {Simultaneous Amplicon Sequencing to Explore Co-Occurrence Patterns of Bacterial, Archaeal and Eukaryotic Microorganisms in Rumen Microbial Communities},
	Ty = {JOUR},
	Volume = {8},
	Year = {2013},
	Bdsk-Url-1 = {http://dx.doi.org/10.1371/journal.pone.0047879}}

