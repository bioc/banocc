	Journal = {PLoS ONE},
	Keywords = {Dirichlet-Multinomial},
	Month = {02},
	Number = {2},
	Pages = {1-15},
	Publisher = {Public Library of Science},
	Title = {Dirichlet Multinomial Mixtures: Generative Models for Microbial Metagenomics},
	Url = {http://dx.doi.org/10.1371%2Fjournal.pone.0030126},
	Volume = {7},
